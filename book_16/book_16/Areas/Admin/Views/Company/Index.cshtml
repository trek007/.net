@model book.models.Campany
<div class="row">
    <div class="col-9">
        <h2 class="text-info">Company</h2>
    </div>
    <div class="col-3">
        <a asp-action="Upsert" class="btn btn-info">
            <i class="fas fa-plus"></i>&nbsp; New Company
        </a>
    </div>
</div>
<div class="border p-2 m-2">
    <table id="tblData" class=" table table-striped table-active table-bordered border-bottom">
        <thead>
            <tr>
                <th>Name</th>
                <th>Address</th>
                <th>City</th>
                <th>State</th>
                <th>PhoneNumber</th>
                <th>st/ph</th>
                <th>IsAuthorized</th>
                <th>Action</th>
            </tr>
        </thead>
    </table>
</div>
@section scripts
{
    <script src="~/js/Campany.js"></script>
}
