<div class="row">
    <div class="col-9">
        <h2 class="text-primary">UserList</h2>
    </div>
    <div class="col-3">
        <a asp-area="Identity" class="btn btn-info" asp-page="/Account/Register">
            <i class="fas fa-plus"></i> &nbsp;
            Create New User
        </a>
    </div>
</div>
<div class="border p-2 m-2">
    <table id="tblData" class="table table-striped table-bordered">
        <thead>
            <tr>
                <th>Name</th>
                <th>Email</th>
                <th>PhoneNumber</th>
                <th>Campany</th>
                <th>Role</th>
                <th>Action</th>
            </tr>
        </thead>
    </table>
</div>
@section scripts
{
    <script src="~/js/User.js"></script>
}

